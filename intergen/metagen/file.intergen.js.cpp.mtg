static void throw_js_error( v8::TryCatch& tc, const char* str )
{
    v8::HandleScope handle_scope;
    v8::String::Utf8Value exc(tc.Exception());

    v8::Handle<v8::Message> message = tc.Message();
    if(message.IsEmpty()) {
        throw exception() << str << *exc;
    }
    else {
        v8::String::Utf8Value filename(message->GetScriptResourceName());
        const char* filename_string = *filename;
        int linenum = message->GetLineNumber();

        throw exception() << filename_string << '(' << linenum << "): " << *exc;
    }
}

static v8::Handle<v8::Value> js_local_include(const v8::Arguments& args)
{
    if(args.Length() < 1)
        return v8::Undefined();

    v8::String::Utf8Value str(args[0]);
    coid::token path = coid::token(*str, str.length());
    path.trim_whitespace();

    coid::charstr dst;
    if(0 != script_handle::get_target_path(path, 0, "", dst, true))
        return v8::ThrowException(v8::Exception::Error(v8::String::New("invalid path")));

    if(!dst.ends_with(".js"))
        dst << ".js";

    coid::bifstream bif(dst);
    if(!bif.is_open())
        return v8::ThrowException(v8::Exception::Error(v8::String::New("error opening include file")));

    coid::binstreambuf buf;
    buf.transfer_from(bif);

    coid::token js = buf;

    v8::HandleScope scope;
    v8::Handle<v8::String> source = v8::String::New(js.ptr(), js.len());
    v8::Handle<v8::String> spath  = v8::String::New(dst.ptr(), dst.len());
    v8::Handle<v8::Script> script = v8::Script::Compile(source, spath);

    script->Run();
    return scope.Close(spath);
}


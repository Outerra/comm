        throw_js_error(__trycatch, "$ns rest="::" after="::"$js::$name$::load_script(): ");

    compiled_script->Run();
    if(__trycatch.HasCaught())
        throw_js_error(__trycatch, "$ns rest="::" after="::"$js::$name$::load_script(): ");

    return compiled_script;
}


v8::Handle<v8::Value> $..name$_js_dispatcher::v8creator_$name$$@order$(const v8::Arguments& args, interface_context* ifc)
{
    if(args.Length() < 1+$ninargs_nondef$ || args.Length() > 1+$ninargs$)  //fnc name + in/inout arguments
        return v8::ThrowException(v8::String::New("Wrong number of arguments"));

    v8::HandleScope _handle_scope_;
    v8::Local<v8::Object> _obj_ = args.Holder();

    //$..ns rest="::" after="::"$js::$..name$_js_dispatcher* ifc = static_cast<$..ns rest="::" after="::"$js::$..name$_js_dispatcher*>
    //    (v8::Handle<v8::External>::Cast(_obj_->GetInternalField(0))->Value());

    v8::Context::Scope context_scope(ifc->_context);

    //stream the arguments in
$-[args inarg? !nojs?]$
 $-(if iref.true?)$
  $-(if !defval.empty?)$
    $basetype$ $name$;
    if($@order$ < args.Length())
        $name$ = ::js::unwrap_object<typename $basetype$::base_t>(args[1+$@order$]);
    else
        $name$ = $defval$;
  $-(elif)$
    $basetype$ $name$ = ::js::unwrap_object<typename $basetype$::base_t>(args[1+$@order$]);
  $-(/if)$
 $-(elif)$
    static_assert( CHECK::meta_operator_exists<$basetype$>::value, "missing metastream operator << for type '$basetype$'" );
    $basetype$ $name$;
  $-(if !defval.empty?)$
    if($@order$ < args.Length())
        v8_streamer<$basetype$>(&ifc->_fmtv8, &ifc->_meta).from_v8(args[1+$@order$], $name$);
    else
        $name$ = $defval$;
  $-(elif)$
    v8_streamer<$basetype$>(&ifc->_fmtv8, &ifc->_meta).from_v8(args[1+$@order$], $name$);
  $-(/if)$
 $-(/if)$
$[/args]-$

    ifc->_meta.stream_acknowledge();

    //invoke
$-[args outarg? !inarg?]$
    $basetype$ $name$;
$[/args]-$

    iref<$..name$_js_dispatcher> nifc = $..ns rest="::" after="::"$$..name$::$name$(new $..name$_js_dispatcher$[args]$, $name$$base2arg$$[/args]$);
    if(nifc)
        nifc->_context = v8::Persistent<v8::Context>::New(ifc->_context);

    //stream out
$-(if return.type!="void")$
    v8::Handle<v8::Value> Null = v8::Null();
 $-(if noutargs?)$
    v8::Handle<v8::Object> _r_ = v8::Object::New();
    _r_->Set(v8::String::NewSymbol("$$ret"), !nifc ? Null : nifc->create_interface_object(true));
 $-(elif)$
    v8::Handle<v8::Value> _r_ = !nifc ? Null : nifc->create_interface_object(true);
 $-(/if)$
$(/if)-$
$-[args outarg?]$
 $-(if iref.false?)$
    static_assert( CHECK::meta_operator_exists<$basetype$>::value, "missing metastream operator << for type '$basetype$'" );
    _r_->Set(v8::String::NewSymbol("$name$"), v8_streamer<$basetype$>(&ifc->_fmtv8, &ifc->_meta).to_v8($name$));
 $-(elif)$
    _r_->Set(v8::String::NewSymbol("$name$"), js_$fulltype$_create_wrapper($name$, ifc->_context));
 $-(/if)$
$[/args]-$

    ifc->_meta.stream_flush();

    return _handle_scope_.Close(_r_);
}

